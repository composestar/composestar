<?xml version="1.0" encoding="UTF-8"?>
<modules xmlns="http://composestar.sourceforge.net/schema/ModuleInfo">
	<moduleinfo id="REXREF" class="Composestar.Core.REXREF.Main">
		<name>Resolve External References</name>
		<description>
			Concerns may have both internal and external references, e.g to a method or a condition represented by an
			object. REXREF traverses the repository and makes sure that all references are resolved.
		</description>

		<incre>
			<module enabled="true" incremental="false" summary="Resolving references...">
				<dependencies></dependencies>
			</module>
		</incre>
	</moduleinfo>
	<moduleinfo id="ParumREXREF" class="Composestar.Core.REXREF.ParumREXREF">
		<name>Resolve selector references</name>
		<incre>
			<module enabled="true" incremental="false" summary="Resolving selector references...">
				<dependencies></dependencies>
			</module>
		</incre>
	</moduleinfo>
</modules>