<?xml version="1.0" encoding="UTF-8"?>
<modules xmlns="http://composestar.sourceforge.net/schema/ModuleInfo">
	<moduleinfo id="INCRESerializer" class="Composestar.Core.INCRE.INCRESerializer">
		<description>Serializes the repository for incremental compilation.</description>
		<settings>
			<setting id="force" type="boolean">
				<name>Force serialization even if INCRE is not enabled.</name>
				<default>True</default>
			</setting>
		</settings>
		<incre>
			<module enabled="true" incremental="false"
				summary="Serializing repository for incremental compilation...">
				<dependencies></dependencies>
			</module>
		</incre>
	</moduleinfo>
</modules>
