<?xml version="1.0" encoding="UTF-8"?>
<modules>
	<moduleinfo id="DIGGER" class="Composestar.Core.DIGGER2.DIGGER">
		<name>Dispatch Graph Generator</name>

		<settings>
			<setting id="resolve" type="boolean">
				<name>Resolve graph</name>
				<default>true</default>
			</setting>
			<setting id="recursionCheck" type="integer">
				<name>Depth of recursion check</name>
				<default>5</default>
			</setting>
			<setting id="exportXml" type="boolean">
				<name>Export to XML file</name>
				<default>true</default>
			</setting>
		</settings>

		<incre>
			<module enabled="true" incremental="false"
				summary="Generating dispatch graph...">
				<dependencies></dependencies>
			</module>
		</incre>
	</moduleinfo>
</modules>
