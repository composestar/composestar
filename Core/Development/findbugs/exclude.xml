<FindBugsFilter>
	<!-- auto generated classes by ANTLR -->
	<Match classregex=".*TreeWalker" />
	
	<Match classregex="Composestar\.Core\.COPPER\.CpsLexer" />
	<Match classregex="Composestar\.Core\.COPPER\.CpsParser" />
	
	<!-- old\obsolete code -->
	<Match classregex="Composestar\.Core\.FIRE\..*" />
	<Match classregex="Composestar\.Core\.SIGN\..*" />
	<Match classregex="Composestar\.Core\.SECRET\..*" />
	
	<Match class="Composestar.Utils.INIFile" />
	
	<Match classregex="Composestar\.DotNET\.LAMA\..*">
		<!-- don't check for leading lowercase in these classes -->
		<BugCode name="Nm" />
	</Match>
</FindBugsFilter>
