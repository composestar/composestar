<FindBugsFilter>
	<!-- auto generated classes by ANTLR -->
	<Match><Class name="~.*TreeWalker" /></Match>
	
	<Match><Class name="Composestar.Core.COPPER.CpsLexer" /></Match>
	<Match><Class name="Composestar.Core.COPPER.CpsParser" /></Match>
	
	<Match><Package name="Composestar.C.wrapper.parsing.GnuC" /></Match>
	<Match><Package name="Composestar.C.wrapper.parsing.StdC" /></Match>
	
	<!-- don't check for leading lowercase in these classes -->
	<Match>
		<Package name="Composestar.DotNET.LAMA" />
		<Bug code="Nm" />
	</Match>
</FindBugsFilter>
