<?xml version='1.0' encoding='utf-8'?> 
<secretfilterdefinition>
	<filters>
		<filter type="Error">
			<accept> <action type="continue"/> </accept>
			<reject> <action type="exception"/> </reject>
		</filter>
		<filter type="Dispatch">
			<accept> <action type="dispatch"/> </accept>
			<reject> <action type="continue"/> </reject>
		</filter>
		<filter type="Wait">
			<accept> <action type="continue"/> </accept>
			<reject> <action type="wait"/> </reject>
		</filter>
		<filter type="Substitute">
			<accept> <action type="substitute"/> </accept>
			<reject> <action type="continue"/></reject>
		</filter>
		<filter type="Meta">
			<accept> <action type="meta"/> </accept>
			<reject> <action type="continue"/></reject>
		</filter>
		<filter type="Realtime">
			<accept> <action type="setrtprop"/> </accept>
			<reject> <action type="continue"/></reject>
		</filter>
		<filter type="Errsubs">
			<accept> <action type="errsubs"/> </accept>
			<reject> <action type="continue"/></reject>
		</filter>
	</filters>
	<actions>
		<action type="continue">
			<operation type="r">
				<resource name="selector"/>
				<resource name="target"/>
				<resource name="state"/>
			</operation>
			<operation type="w">
			</operation>
		</action>
		<action type="exception">
			<operation type="r">
				<resource name="selector"/>
				<resource name="target"/>
				<resource name="state"/>
			</operation>
			<operation type="w">
			</operation>
		</action>
		<action type="dispatch">
			<operation type="r">
				<resource name="selector"/>
				<resource name="target"/>
				<resource name="state"/>
			</operation>
			<operation type="w">
			</operation>
		</action>
		<action type="substitute">
			<operation type="r">
			</operation>
			<operation type="w">
				<resource name="selector"/>
				<resource name="target"/>
				<resource name="state"/>
				<resource name="server"/>
				<resource name="sender"/>
			</operation>
		</action>
		<action type="wait">
			<operation type="r">
				<resource name="selector"/>
				<resource name="target"/>
				<resource name="state"/>
			</operation>
			<operation type="w">
				<resource name="timing"/>
			</operation>
		</action>
		<action type="setrtprop">
			<operation type="r">
				<resource name="selector"/>
				<resource name="target"/>
				<resource name="state"/>
			</operation>
			<operation type="w">
				<resource name="deadline"/>
			</operation>
		</action>
		<action type="meta">
			<operation type="r">
			</operation>
			<operation type="w">
				<resource name="selector"/>
				<resource name="target"/>
				<resource name="state"/>
				<resource name="server"/>
				<resource name="sender"/>
				<resource name="parameters"/>
			</operation>
		</action>
		<action type="System">
			<operation type="r">
				<resource name="selector"/>
				<resource name="target"/>
				<resource name="state"/>
				<resource name="server"/>
				<resource name="sender"/>
				<resource name="parameters"/>
			</operation>
			<operation type="w">
				<resource name="timing"/>
			</operation>
		</action>
		<action type="Scheduler">
			<operation type="r">
				<resource name="selector"/>
				<resource name="target"/>
				<resource name="deadline"/>
				<resource name="timing"/>
			</operation>
			<operation type="w">
			</operation>
		</action>
	</actions>
	<nonconflictstrings>
		<nonconflictstring resourcename="selector">
			<regex string="r*w*"/>
			<alphabet data="rw"/>
		</nonconflictstring>
		<nonconflictstring resourcename="target">
			<regex string="r*w*"/>
			<alphabet data="rw"/>
		</nonconflictstring>
		<nonconflictstring resourcename="sender">
			<regex string="r*w*"/>
			<alphabet data="rw"/>
		</nonconflictstring>
		<nonconflictstring resourcename="server">
			<regex string="r*w*"/>
			<alphabet data="rw"/>
		</nonconflictstring>
		<nonconflictstring resourcename="timing">
			<regex string="r*w*"/>
			<alphabet data="rw"/>
		</nonconflictstring>
		<nonconflictstring resourcename="deadline">
			<regex string="(r*w*)*"/>
			<alphabet data="rw"/>
		</nonconflictstring>
		<nonconflictstring resourcename="state">
			<regex string="r*w*"/>
			<alphabet data="rw"/>
		</nonconflictstring>
		<nonconflictstring resourcename="parameters">
			<regex string="r*w*"/>
			<alphabet data="rw"/>
		</nonconflictstring>
	</nonconflictstrings>
</secretfilterdefinition>
