<?xml version="1.0" encoding="UTF-8"?>
<modules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../../../ComposestarCORE/src/Composestar/Core/Config/Xml/ModuleInfo.xsd">
	<moduleinfo class="Composestar.CwC.COMP.PreProcess" id="PreCOMP">
		<name>C PreProcessor</name>
		<description>Calls the C preprocessor</description>
		<settings>
			<setting id="preprocessed" type="boolean">
				<name>PreProcessed input</name>
				<default>true</default>
			</setting>
		</settings>
	</moduleinfo>
	<moduleinfo class="Composestar.CwC.COMP.Compile" id="COMP">
		<name>C Compiler</name>
		<description>Calls the C Compiler</description>
		<settings>
			<setting id="args" type="string">
				<name>Additional compiler arguments</name>
			</setting>
			<setting id="nocompile" type="boolean">
				<name>Do not compile</name>
				<default>true</default>
			</setting>
		</settings>
	</moduleinfo>
</modules>
