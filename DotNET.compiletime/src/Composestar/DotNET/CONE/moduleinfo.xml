<?xml version="1.0" encoding="UTF-8"?>
<modules>
	<moduleinfo id="CONE-IS" class="Composestar.DotNET.CONE.DotNETWeaveFileGenerator">
		<description>Generates the weaving specification file.</description>
		<incre>
			<module enabled="true" incremental="false" summary="Generating weave specification file...">
				<dependencies></dependencies>
			</module>
		</incre>
	</moduleinfo>

	<moduleinfo id="CONE-XML" class="Composestar.DotNET.CONE.DotNETRepositorySerializer">
		<description>Saves the whole repository to an XML so that it's available at runtime.</description>
		<settings>
			<setting id="compressed" type="boolean">
				<name>Compress file</name>
				<default>true</default>
			</setting>
		</settings>
		<incre>
			<module enabled="true" incremental="false" summary="Serializing repository...">
				<dependencies></dependencies>
			</module>
		</incre>
	</moduleinfo>
</modules>
